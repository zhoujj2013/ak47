<script type="text/javascript">
    var SEARCH_INTERPRO = "Search InterPro...";

    function displaySearchInterPro(queryEl) {
        if (queryEl == null) {
            return;
        }
        if (queryEl.value == null || queryEl.value.trim() == "") {
            queryEl.value = SEARCH_INTERPRO;
        }
    }

    function hideSearchInterPro(queryEl) {
        if (queryEl == null || queryEl.value == null) {
            return;
        }
        if (queryEl.value.trim() == SEARCH_INTERPRO) {
            queryEl.value = "";
        }
    }

    function loaded() {
        displaySearchInterPro(document.getElementById('local-searchbox'));
    }
</script>

<!--Back top jQuery plugin-->
   <script type="text/javascript">
       $(function () {
           $(window).scroll(function () {
               if ($(this).scrollTop() != 0) {
                   $('#toTop').fadeIn();
               } else {
                   $('#toTop').fadeOut();
               }
           });

           $('#toTop').click(function () {
               $('body,html').animate({scrollTop:0}, 800);
           });
       });
   </script>


